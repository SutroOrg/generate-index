{
  "name": "generate-ts-index",
  "displayName": "generate-ts-index",
  "description": "Generate a typescript index file that exports from all files in a directory",
  "version": "0.0.1",
  "publisher": "kingdaro",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:extension.generateIndex"],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateIndex",
        "title": "Generate TS index file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.1.3",
    "tslint": "^5.8.0",
    "typescript": "^2.8.3",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "util.promisify": "^1.0.0"
  }
}
